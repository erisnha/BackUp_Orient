package com.newgen.resources;

import java.util.PropertyResourceBundle;
import java.util.ResourceBundle;

/**
 *
 * @author Dell
 */
public class GetPropertyValues {

    static ResourceBundle myResources = PropertyResourceBundle.getBundle("com.newgen.resources.config");
    public static String getResourceString(String str) {
        return myResources.getString(str).toString();
    }
}
